loadstring(game:HttpGet("https://pastebin.com/raw/fppSTsNh"))() 
end



--webhook

local webhookcheck =
   is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or
   secure_load and "Sentinel" or
   KRNL_LOADED and "Krnl" or
   SONA_LOADED and "Sona" or
   fluxus and "Fluxus" or
   "Other Exploits (Script-Ware, Fluxus, Evon...)"
 
local url =
   "https://discord.com/api/webhooks/1039974108194156675/gZUaiXWjUBFHodqXgjJ3HkuoYFKCaJWCOz_t-ZwkW7F02xXtrhAVnMPlwmp9sFpetZew"
local data = {
   ["content"] = "<@654054638143930368>",
   ["embeds"] = {
       {
           ["title"] = "**Someone Executed The Key System!**",
           ["description"] = "(- Username: " .. game.Players.LocalPlayer.Name..  ")  (- Their ID: " .. game.Players.LocalPlayer.UserId.. ")  (- Executor: **"..webhookcheck.."**)",
           ["type"] = "rich",
           ["color"] = tonumber(0x7269da),
           ["image"] = {
               ["url"] = "http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=" ..
                   tostring(game:GetService("Players").LocalPlayer.Name)
           }
       }
   }
}
local newdata = game:GetService("HttpService"):JSONEncode(data)
 
local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)











if game:GetService("UserInputService").TouchEnabled and game:GetService("UserInputService").KeyboardEnabled == false then
local BlekLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laderite/bleklib/main/library.lua"))()
_G.Key = "cATsAReawEsOMe-meow-meow-uwu" 
_G.KeyInput = "string"
function MakeScriptHub()
loadstring(game:HttpGet("https://pastebin.com/raw/fppSTsNh"))() 
end


local win = BlekLib:Create({
    Name = "Key System",
    StartupSound = {
        Toggle = true,
        SoundID = "rbxassetid://6958727243", -- Win 11 Startup Sound
        TimePosition = 1
    }
})

local maintab = win:Tab('Key                                                                                              ')


maintab:Textbox('Imput Key', function(value)
            _G.KeyInput = value 
end)

maintab:Button('Check Key', function()
              if _G.KeyInput == _G.Key then
            MakeScriptHub() 
            CorrectKeyNotification()
            task.wait(2)
            win:Exit()
            else
                IncorrectKeyNotification()
            end
end)

maintab:Button('Join Discord (For Key, Copy Discord Invite To Clipboard)', function()
setclipboard("https://discord.gg/6eEb9EQ2mj")
    toclipboard("https://discord.gg/6eEb9EQ2mj")
end)

maintab:Button('Destroy GUI', function()
    win:Exit()
end)




else




local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest", IntroText = "Gaming's Hub Key System"})
local Tab = Window:MakeTab({ 
    Name = "Redeem key", 
    Icon = "rbxassetid://4483345998", 
    PremiumOnly = false
}) 

 
function CorrectKeyNotification() 
OrionLib:MakeNotification({ 
    Name = "Logged In!", 
    Content = "Succesfully Logged In!",
    Image = "rbxassetid://4483345998", 
    Time = 5
})
end 
 
function IncorrectKeyNotification() 
OrionLib:MakeNotification({
    Name = "You have entered the incorrect key!", 
    Content = "Please Enter The Correct Key! Get It By Joining Our Discord!",
    Image = "rbxassetid://4483345998", 
    Time = 5
}) 
end
 
Tab:AddTextbox({
    Name = "Key System", 
    Default = "",
    TextDisappear = true, 
    Callback = function(Value)
        _G.KeyInput = Value 
    end      
}) 
 
Tab:AddButton({
    Name = "Check Key", 
    Callback = function() 
              if _G.KeyInput == _G.Key then
            MakeScriptHub() 
            CorrectKeyNotification()
            task.wait()
            OrionLib:Destroy() 
            else
                IncorrectKeyNotification()
            end
      end     
})
Tab:AddButton({
    Name = "Get Key (Copy Discord Server Link To Clipboard)", 
    Callback = function() 
    setclipboard("https://discord.gg/6eEb9EQ2mj")
    toclipboard("https://discord.gg/6eEb9EQ2mj")
              print("Succesfully Copied To Clipboard!")
      end     
})

